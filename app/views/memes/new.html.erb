<h1>New Meme</h1>

<%= form_with model: @meme, url: memes_path do |f| %>
  <div>
    <%= f.label :image %><br>
    <%= f.file_field :image %>
  </div>

  <div>
    <%= f.radio_button :lang, 'en' %> 
    <%= f.label :lang, 'en', :value => 'en' %>
    <%= f.radio_button :lang, 'de' %>
    <%= f.label :lang, 'de', :value => 'de' %>
  </div>

  <div>
    <%= f.submit %>
  </div>
<% end %>